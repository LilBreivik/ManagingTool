{
    "version": "0.2.0",
    "configurations": [

      {
        "name": "ng serve",
        "type": "chrome",
        "request": "launch",
        "url": "http://127.0.0.1:4200/#",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "ng test",
        "type": "chrome",
        "request": "launch",
        "url": "http://127.0.0.1:9876/debug.html",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "ng e2e",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
        "protocol": "inspector",
        "args": ["${workspaceFolder}/e2e/protractor.conf.js"]
      }
    ]
  }