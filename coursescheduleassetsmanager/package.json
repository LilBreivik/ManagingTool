{
  "name": "coursescheduleassetsmanager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "copyUploaderJS": "cp -rp js/uploader.js ../core/src/main/resources/static/js",
    "removeAngularCLIAssets": "rm dist/coursescheduleassetsmanager/3rdpartylicenses.txt && rm dist/coursescheduleassetsmanager/favicon.ico",
    "copyCourseScheduleAssetsHTMLApplication": "mv dist/coursescheduleassetsmanager/index.html ../core/src/main/resources/static/templates/fragments/coursescheduleassetsmanager.html",
    "copyCourseScheduleAssetsJSApplicaton": "cp -rp dist/coursescheduleassetsmanager ../core/src/main/resources/static/js",
    "loadCourseScheduleAssetsManagerApplication": "ng build --configuration=production --aot --build-optimizer=false  && npm run removeAngularCLIAssets && npm run copyCourseScheduleAssetsHTMLApplication && npm run copyCourseScheduleAssetsJSApplicaton && npm run copyUploaderJS",
    "build": "ng build --configuration=production --source-map",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@types/file-saver": "^1.3.0",
    "@types/jquery": "^3.3.6",
    "angular-file": "^0.5.4",
    "core-js": "^2.5.4",
    "file-saver": "^1.3.8",
    "jquery": "^3.3.1",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
